namespace Eorzea;

table WeatherRate {
	weathers: [uint8];
	rates: [uint8];
}

table WeatherMap {
	region: uint8;
	place: uint8;
	rate: WeatherRate;
}

table FishingSpot {
	x: uint16;
	z: uint16;
	radius: uint16;
	spotId: uint8;
	regionId: uint8;
	placeId: uint8;
	spot_category: uint8;
}

table FishingItem {
	itemId: uint16;
	spots: [FishingSpot];
}

table DB {
	fishingItems: [FishingItem];
	weatherMaps: [WeatherMap];
}

table TextDB {
	itemNames: [string];
	placeNames: [string];
	weatherNames: [string];
}

